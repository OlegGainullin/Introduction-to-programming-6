#include <stdio.h>
#include <locale.h>
#include <stdlib.h>
int main()
{
    char *locale = setlocale(LC_ALL, "");
    float x = 0;
    printf("Введите x: ");
    scanf("%e", &x);
    float y = 4*((x-3)*(x-3)*(x-3)*(x-3)*(x-3)*(x-3))-7*(x-3)*(x-3)*(x-3)+2;
    printf("y = %g \n", y);
    return 0;
}
