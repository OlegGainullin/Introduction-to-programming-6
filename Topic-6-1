#include <stdio.h>
#include <locale.h>
#include <stdlib.h>
int main()
{
    char *locale = setlocale(LC_ALL, "");
    float a = 0;
    float b = 0;
    printf("Введите A: ");
    scanf("%e", &a);
    printf("Введите B: ");
    scanf("%e", &b);
    float c = 0;
    c = a;
    a = b;
    b = c;
    printf("A = %g \n", a);
    printf("B = %g \n", b);
    return 0;
}
